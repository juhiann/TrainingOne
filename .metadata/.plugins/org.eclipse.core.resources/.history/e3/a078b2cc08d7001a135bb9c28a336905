package projectTwelve;

public class MyNumber 
{

	//creating variables
	public int number;
	
	//constructor
	public MyNumber(int number) {
		this.number = number;
	}

	
	//additional methods
	public boolean isPrime()
	{
		if(number %2 == 0)
		{
			return false;
		}
		else 
			return true;
		
	}
	
	
	

	
	
	
	



	
	

	
}
	

	
		
