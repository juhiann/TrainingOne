package projectTwelve;

public class MyNumber 
{

	//creating variables
	public int number;
	
	//constructor
	public MyNumber(int number) {
		this.number = number;
	}

	
	//additional methods
	
	public boolean isPrime()
	{
		if(number < 2)
		{
			return false;
		}
		
		for(int i = 2; i<= number - 1; i++)
		{
			if(number %i ==0)
			{
				return false;
			}
		}
		
		return true;
	}

	
	public int sumUptoN() 
	{
		for(int i = 1; i <= number; i++)
		{
			int x = i;
			
			
		
			
		}
		return 0;
	}


	
	
	
	



	
	

	
}
	

	
		
